            </div>\n                        <hr/>\n                        <div className=\"row\">\n                            <div className=\"col-md-4 mb-3\"><button type=\"button\" className=\"btn btn-warning\" onClick={()=>history.push('/')}><i className=\"fas fa-cart-plus\"></i> Continuer les achats</button></div>\n                            <div className=\"col-md-4 mb-3\"><button type=\"button\" className=\"btn btn-secondary\" onClick={()=>setState(!state)}><i className=\"fas fa-sync-alt\"></i> Actualiser le panier</button></div>\n                            <div className=\"col-md-4 mb-3\"> <button type=\"button\" className=\"btn btn-secondary\" onClick={delAll}><i className=\"fas fa-trash\"></i> Tout Supprimer</button></div>\n                        </div>\n                    </div>\n                    <div className=\"col-md-3 resume\">\n                        <h4>RESUME</h4>\n                        <ul className=\"list-group\">\n                            <li className=\"list-group-item d-flex justify-content-between align-items-center\">\n                                Nombre D'articles\n                                <span className=\"badge badge-primary badge-pill\"> {nbrArt()} </span>\n                            </li>\n                            <li className=\"list-group-item d-flex justify-content-between align-items-center\">\n                                Montant total\n                                <span className=\"badge badge-primary badge-pill\"> {total()} Fcfa </span>\n                            </li>\n                            <li className=\"list-group-item d-flex justify-content-between align-items-center\">\n                                Taxe\n                                <span className=\"badge badge-primary badge-pill\"> 0 Fcfa </span>\n                            </li>\n                            <li className=\"list-group-item d-flex justify-content-between align-items-center\">\n                                Total de la commande\n                                <span className=\"badge badge-primary badge-pill\"> {total()-taxe} Fcfa </span>\n                            </li>\n                            <button className=\"btn btn-secondary colorbtncommander\" onClick={sendComand}><b>COMMANDER</b></button>\n                        </ul>\n                    </div>\n                </div>\n            </div>\n        </div>\n    );\n\n}\n\nconst mapStateToProps=(state: any)=>({\n    comand: state.comand,\n})\n\nexport default connect(mapStateToProps)(Panier);","/media/bbh/@Nkul/PROJET_CLASSE_X/innov237/consoInvest_web/src/component/pages/Product/ProductDetail/ProductDetail.tsx",["182"],"import React from 'react';\nimport './ProductDetail.css'\n\nconst ProductDetail: React.FC = () => {\n    return (\n        <div className=\"container\">\n            <div className=\"row container-fluid\">\n                <div className=\"col-md-6\">\n                    <div className=\"row\">\n                        <div className=\"col-12\">\n                            <img width=\"100%\" src=\"https://global2019-static-cdn.kikuu.com/upload-productImg-1557817317157_320_234.jpeg?\" />\n                        </div>\n                    </div>\n                </div>\n\n                <div className=\"col-md-6 product__detail__row\">\n                    <p className=\"product__title\">Chausure homme</p>\n                    <p className=\"product__price\">50.000 FCFA</p>\n                    <p className=\"product__decription\">Lorem ipsum dolor sit amet consectetur adipisicing elit. Recusandae hic alias nemo illo, illum laboriosam libero similique at, deleniti quo repudiandae voluptatem? Aut qui, iusto quia ipsam nisi dolor incidunt!</p>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default ProductDetail;","/media/bbh/@Nkul/PROJET_CLASSE_X/innov237/consoInvest_web/src/component/pages/User/Login/Login.tsx",["183","184"],"import React from \"react\"