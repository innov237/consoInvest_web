lassName=\"fas fa-cog fa-spin\"></i>\n                </button>\n            </nav>\n            <div className=\"container\">\n                <div className=\"row\">\n                    <div className=\"col-md-2 no-padding left__menu\">\n                        <Sidebar />\n                    </div>\n                    <div className=\"col-md-8 p-3\">\n                        <Switch>\n                            <Route path=\"/account\" exact>\n                                <Profil />\n                            </Route>\n                        </Switch>\n                        <Switch>\n                            <Route path=\"/account/user\">\n                            <Sidebar />\n                            </Route>\n                        </Switch>\n                        <Switch>\n                            <Route path=\"/account/commande\">\n                                <Commande />\n                            </Route>\n                        </Switch>\n                        <Switch>\n                            <Route path=\"/account/affiliers\">\n                                <Affiliers />\n                            </Route>\n                        </Switch>\n                        <Switch>\n                            <Route path=\"/account/password\">\n                                <Password />\n                            </Route>\n                        </Switch>\n                        <Switch>\n                            <Route path=\"/account/boutique\">\n                                <Boutique />\n                            </Route>\n                        </Switch>\n                        <Switch>\n                            <Route path=\"/account/epagne\">\n                                <Epagne />\n                            </Route>\n                        </Switch>\n                        \n                </div>\n\n                <div className=\"col-md-2  p-3\">\n                    <div className