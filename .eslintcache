vigation'\n\n\nimport { useDispatch, useSelector } from 'react-redux'\n\nimport { LOGIN_ACTION,SHOP_ACTION } from './store/authReducers'\n\n\n/**j'utilise les function component au lieu des class component l'execution est plus rapide*/\nconst  App = () => {\n\n \n  return (\n    <ApiContext.Provider value={new ApiService()} >\n      <StorageContext.Provider value={new StorageService()} >\n        <Router>\n          <Provider store={store} >\n            <div className=\"App\">\n              <Navbar />\n                <Navigation /> \n            </div>\n          </Provider>\n        </Router>\n    </StorageContext.Provider>\n    </ApiContext.Provider>\n  );\n}\n\nexport default App;\n","/media/bbh/@Nkul/PROJET_CLASSE_X/innov237/consoInvest_web/src/store/configStore.js",[],["169","170"],"/media/bbh/@Nkul/PROJET_CLASSE_X/innov237/consoInvest_web/src/context/ApiContext.tsx",[],"/media/bbh/@Nkul/PROJET_CLASSE_X/innov237/consoInvest_web/src/services/StorageService.tsx",[],"/media/bbh/@Nkul/PROJET_CLASSE_X/innov237/consoInvest_web/src/services/ApiService.tsx",[],"/media/bbh/@Nkul/PROJET_CLASSE_X/innov237/consoInvest_web/src/context/StorageContext.tsx",["171"],"import React from 'react'\nimport StorageService from '../services/StorageService'\nconst StorageContext=React.createContext(new StorageService)\n\nexport default StorageContext","/media/bbh/@Nkul/PROJET_CLASSE_X/innov237/consoInvest_web/src/component/navigation/Navigation.tsx",[],"/media/bbh/@Nkul/PROJET_CLASSE_X/innov237/consoInvest_web/src/component/layout/Navbar/Navbar.tsx",["172","173","174"],"import React from \"react\";\nimport { Link, useHistory } from \"react-router-dom\";\nimport \"./Navbar.css\";\nimport Search from './Search'\n\nimport { useSelector, useDispatch} from 'react-redux'\nimport { AnyAction } from \"redux\";\n\n\nimport ApiService from \"../../../services/ApiService\"; \n\n\n\nimport {LOGIN_ACTION,SHOP_ACTION} from '../../../store/authReducers'\n\nconst Navbar: React.FC = (props) => {\n   \n    const history=useHistory()\n\n    const Api = new ApiService();\n\n    const cmds = useSelector((state:any) => state.comand) \n    const auth = useSelector((state:any)=> state.auth)\n    const dispatch = useDispatch()\n\n\n    const panier:any = cmds.items\n\n    console.log(cmds)\n\n    const getShop = async (id:any) => {\n        var response = await Api.getData(\"getUserShop?id_user=\"+id);\n        if (response.data.length)\n            dispatch(SHOP_ACTION(response.data[0]))\n        \n    }\n    \n    const initialize = () => {\n        const auth = localStorage.getItem(\"authUserData\");\n        if (auth){\n            dispatch(LOGIN_ACTION(JSON.parse(auth)))\n            getShop(JSON.parse(auth).id)\n        }\n        \n    }\n\n\n    React.useEffect(() => {\n        //initialize()\n    },[])\n\n\n    return (\n        <div>\n            <nav className=\"navbar shadow-sm navbar-expand-lg\">\n                <div className=\"container d-flex justify-content-center align-items-center\">\n                    <div className=\"row container-fluid\">\n                        <div className=\"col-md-2 col-sm-12 no__padding\">\n                            <Link to=\"/home\">\n                                <img width=\"70px\" className=\"logo__image\" src=\"data:image/jpeg;base64,\" alt=\"logo\" />\n                            </Link>\n                        </div>\n                        <div className=\"col-md-8 col-sm-12\">\n                            <Search />\n                        </div>\n                        <div className=\"col-md-2 col-sm-12\">\n                            <div className=\"row\">\n                                <div className=\"col-6\">\n                                    <Link to=\"/panier\">\n                                    <div className=\"baket__item\">\n                                        <img className=\"shoping__basket\" src=\"../../../../images/shopping-cart.png\" alt=\"\" />\n                                        <span>{panier}</span>\n                                        <p>Panier</p>\n                                    </div>\n                                    </Link>\n                                </div>\n                                \n                                <div className=\"col-6\" style={{ 'display' : (auth.user) ? 'block' : 'none'}}>\n                                    <Link to=\"/account\">\n                                         <img className=\"shoping__basket\" src=\"../../../../images/user.png\" alt=\"\" />\n                                        <p>Compte</p>\n                                    </Link>\n                                </div>\n                                \n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </nav>\n        </div>\n    )\n}\n\nexport default Navbar;","/media/bbh/@Nkul/PROJET_CLASSE_X/innov237/consoInvest_web/src/store/comandReducers.js",["175"],"/media/bbh/@Nkul/PROJET_CLASSE_X/innov237/consoInvest_web/src/component/layout/Navbar/Search.tsx",["176","177","178","179","180"],"/media/bbh/@Nkul/PROJET_CLASSE_X/innov237/consoInvest_web/src/component/pages/Detail_produit/Detail_produit.tsx",["181","182","183","184","185","186","187","188","189"],"import React, { useEffect, useState } from \"react\";\nimport { useHistory } from \"react-router-dom\";\nimport { connect, useDispatch, useSelector } from 'react-redux'\n\nimport ApiService from \"../../../services/ApiService\";\nimport './Detail_produit.css';\nimport { act } from \"react-dom/test-utils\";\n\nimport queryString from 'query-string';\n\n\n\nconst Detail: React.FC = () => {\n\n    const history = useHistory();\n    const item = useSelector((state:any) => state.search.item)\n    const [productData, setProduct] = useState<any>([]);\n    const [slider, setSlider] = useState<any>([]);\n    const [isload, setLoader] = useState(false);\n    const [quantity, setQuantity]=useState(1)\n    const Api = new ApiService();\n    let dispatch=useDispatch()\n    \n    const comand=useSelector((state: any)=> state.comand)\n    const auth = useSelector((state:any) => state.auth)\n\n \n    const toArray = (data: any) => {\n        return JSON.parse(data)\n    }\n\n\n    const process= async (parsed:any)=>{\n       \n\n        if (parsed.slug){\n            var response = await Api.getData(`getPublicationBySlug?key=${parsed.slug}&?lastInsertId=10`);\n            if (response.status == 200){\n\n                if ( response.data.length){\n                    setProduct(response.data[0])\n                }else{\n                    alert(`No data found for ${parsed.params}`);\n                    history.push('/home');\n                };\n            }\n\n        };\n        \n    }\n\n\n    const command = async () => {\n\n        const credentials = {\n            'id_user':auth.user.id,\n            'id_boutique':productData.id_boutique,\n            'produits': [productData],\n            'montant_cmd': quantity * productData.prix\n        }\n\n        console.log(credentials)\n\n        const response = await Api.postData('enregistrerCommande', credentials)\n        \n        if (response.status == 200) {\n            alert('Opération effectuée avec success')\n            history.push('/home')\n            \n        }\n    }\n\n  \n    const changeQuantity=(num: number)=>{\n        setQuantity((quantity+num)? quantity+num : 1)\n    }\n    const comandItem=()=>{\n        const action={\n            type: 'ADD_COMAND_ITEM',\n            value: {\n                item: productData,\n                quantity: quantity\n            }\n        }\n        dispatch(action)\n    }\n\n\n    useEffect(() => {\n        if(item)\n            setProduct(item)\n        else\n            process(queryString.parse(history.location.search))\n\n        setLoader(true);\n    }, [])\n    \n    return (\n        <div>\n            <div className=\"container\">\n                <div className=\"row\">\n                    <div className=\"col-md-6 left__menu\">\n                        <div id=\"carouselExampleInterval\" className=\"carousel slide\" data-ride=\"carousel\">\n                            <div className=\"carousel-inner\">\n                                { \n                                    (productData.images) ?\n                                    (\n                                        toArray(productData.images).map((result: any) => (\n                                            <div className=\"carousel-item active\" data-interval=\"1000\">\n                                                <img src={Api.imageUrl + result} className=\"d-block w-100\" />\n                                            </div>\n                                            )\n                                        )\n                                    ) : <div></div>\n                                }\n                                {/* {toArray(productData['images'])} */}\n                            </div>\n                            <a className=\"carousel-control-prev\" href=\"#carouselExampleInterval\" role=\"button\" data-slide=\"prev\">\n                                <span className=\"carousel-control-prev-icon\" aria-hidden=\"true\"></span>\n                                <span className=\"sr-only\">Previous</span>\n                            </a>\n                            <a className=\"carousel-control-next\" href=\"#carouselExampleInterval\" role=\"button\" data-slide=\"next\">\n                                <span className=\"carousel-control-next-icon\" aria-hidden=\"true\"></span>\n                                <span className=\"sr-only\">Next</span>\n                            </a>\n                        </div>\n                    </div>\n                    <div className=\"col-md-6\">\n                        <div className=\"card mb-3\">\n                            <div className=\"card-body\">\n                                <h5 className=\"card-title\">{productData[\"titre\"]}</h5>\n                                <p className=\"card-text\">\n                                    {productData[\"description\"]}\n                                </p>\n                                <div className=\"alert alert-primary alert-link\" role=\"alert\">\n                                    {productData[\"prix\"]} CFA\n                            </div>\n                                <ul className=\"list-group list-group-flush\">\n                                    Quantité:\n                                    <div className=\"btn-group\" role=\"group\" aria-label=\"Basic example\">\n                                        <button type=\"button\" className=\"btn btn-secondary\" onClick={()=>changeQuantity(-1)}><b>-</b></button>\n                                        <input type=\"text\" name=\"\" id=\"\" value={quantity} onChange={()=>{}}/>\n                                        <button type=\"button\" className=\"btn btn-secondary\" onClick={()=>changeQuantity(1)}><b>+</b></button>\n                                    </div>\n                                </ul>\n                                <br />\n                                <div className=\"btn-toolbar mb-3\" role=\"toolbar\" aria-label=\"Toolbar with button groups\">\n                                    <div className=\"btn-group mr-2\" role=\"group\" aria-label=\"First group\">\n                                        <button type=\"button\" className=\"btn btn-secondary\" onClick={() => command()} style={{'display' : (auth.user.id) ? 'block': 'none'}}>Acheter</button>\n                                    </div>\n                                    <div className=\"input-group\">\n                                        <button type=\"button\" className=\"btn btn-secondary\" onClick={comandItem}><i className=\"fas fa-cart-plus\"></i> Ajouter au panier</button>\n                                    </div>\n                                </div>\n                                <p className=\"card-text\"><small className=\"text-muted\">Last updated 3 mins ago</small></p>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n\n                <div className=\"card border-primary mb-3 w-100\">\n                    <div className=\"card-header\"></div>\n                    <div className=\"card-body text-secondary\">\n                        <u><p className=\"card-title description\">Description du produit</p></u>\n                        <div className=\"card-text\">\n                            <ul className=\"row\">\n                                <li className=\"col-md-6 no-padding\">Categorie : {productData[\"libelle_categorie\"]} </li>\n                                <li className=\"col-md-6 no-padding\">Ville : {productData[\"ville\"]} </li>\n                                <li className=\"col-md-6 no-padding\">Sexe : {productData[\"sexe\"]} </li>\n                                <li className=\"col-md-6 no-padding\">Telephone : {productData[\"telephone\"]} </li>\n                                <li className=\"col-md-6 no-padding\">Age : {productData[\"age\"]} </li>\n                                <li className=\"col-md-6 no-padding\">Lieu Boutique : {productData[\"lieu_boutique\"]} </li>\n                            </ul>\n                            <div className=\"row justify-content-center\">\n                                <button className=\"col-3 btn btn-success\">Livraison</button>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div >\n    );\n\n}\nconst mapStateToProps=(state: any)=>({\n    comand: state.comand,\n});\n\n\nexport default connect(mapStateToProps)(Detail);\n","/media/bbh/@Nkul/PROJET_CLASSE_X/innov237/consoInvest_web/src/component/pages/Panier/Panier.tsx",["190","191","192","193","194","195","196"],"import React, { useState } from \"react\";\nimport { connect, useSelector, useDispatch } from 'react-redux'\n\nimport { useHistory } from 'react-router-dom'\n\nimport './Panier.css';\nimport ApiService from '../../../services/ApiService'\n\nvar idDel: any=[]\n\nconst Panier: React.FC = ({history}: any) => {\n    const [state, setState]=useState(false)\n    const [check, setCheck]=useState(false)\n    const comand=useSelector((state: any)=>state.comand)\n    const auth=useSelector((state: any)=>state.auth.user)\n    const dispatch = useDispatch()\n    const Api=new ApiService();\n\n    const nav = useHistory()\n\n\n  \n    const getBoutique: any = () => {\n\n        const bt = Array.from(new Set(comand.cmds.map((i:any)=> i.item.id_boutique)))\n\n         const all:any = bt.map((i:any) => {\n             return {\n                produits: [],\n                id_boutique: i,\n                id_user: auth.id,\n                montant_cmd: 0\n            }\n         })\n\n        return all\n\n    }\n\n    const nbrArt=()=>{\n        let nbre=0\n        comand.cmds.map((data: any)=>nbre=nbre+data.quantity)\n        return nbre\n    }\n    const total=()=>{\n        let total=0\n        comand.cmds.map(({item, quantity}: any)=>total=total+item.prix*quantity)\n        return total\n    }\n\n    const taxe=0\n    const addDel=(id: number)=>idDel.push(id)\n    const removeDel=(id: any)=>idDel=idDel.filter((data: any)=>data!==id)\n    const delItem=(id: any)=>{\n        const action={\n            type: 'REMOVE_COMAND_ITEM',\n            value: id\n        }\n        dispatch(action)\n        setState(!state)\n    }\n    const delAll=()=>{\n        idDel.map((id: any)=>{\n            const action={\n                type: 'REMOVE_COMAND_ITEM',\n                value: id \n            }\n            dispatch(action)\n        })\n        idDel=[]\n        setCheck(false)\n        console.log(comand)\n    }\n    const handleCheck=(e: any, id: any)=>{\n        e.target.checked ? addDel(id) : removeDel(id) \n        \n    }\n    const checkAll=(e: any)=>{\n        setCheck(e.target.checked)\n        if(e.target.checked) comand.map(({item}: any)=>idDel.push(item.id))\n        else idDel=[]\n    }\n    const updateQte = (e:any, item:any) =>{\n        console.log(e, item)\n\n        let qte = e\n\n        if (!e)\n            qte = 1\n\n\n        dispatch({\n            type: 'UPDATE_QTE',\n            payload:{\n                qte,\n                item,\n            }\n        })\n\n    }\n    const sendComand=()=>{\n        const current = getBoutique()\n\n        \n\n        const save = comand.cmds.map(({item, quantity}: any)=>{\n            let find = undefined\n            let com={\n                id: item.id_publication,\n                panierId: '0',\n                titre: item.titre,\n                prix: item.prix,\n                quantite: quantity,\n                descripton: item.description,\n                image: item.images,\n                id_boutique: item.id_boutique\n            }\n\n            for (var i = 0; i < current.length; i++) {\n              if (item.id_boutique == current[i].id_boutique) {\n                find = i ;\n                break;\n              }\n            }\n\n            \n            if (find != undefined){\n                current[find].produits = [...current[find].produits, com]\n                current[find].montant_cmd = current[find].montant_cmd + item.prix*quantity\n               \n            }\n        })\n        \n        \n        current.map((e:any) => Api.postData('enregistrerCommande', e)) \n\n        alert('Commande effectuée')\n        dispatch({ type: 'RESET'})\n       }\n\n    \n    const toggle = () => (comand.length && auth.user && auth.user.id) ? true: false;\n\n    const listItem=comand.cmds.length ? (\n        comand.cmds.map(({item, quantity}: any,)=>\n        <tr>\n            <th scope=\"row\"> { check ? <input type=\"checkbox\" checked={check}/> : <input type=\"checkbox\" onClick={e=>handleCheck(e, item.id)}/>} </th>\n            <th><img className=\"rounded-circle p-3\" width=\"60%\" src=\"https://mdbootstrap.com/img/Photos/Avatars/img%20(30).jpg\" data-holder-rendered=\"true\"/></th>\n            \n            <td>{item.nom}</td>\n            <td>{item.description}</td>\n            <td><input type=\"number\"  value={quantity}  onChange={(e) => updateQte(e.target.value,item)}/></td>\n            <td>{item.prix}FCFA</td>\n            <td className=\"trash\" onClick={()=>delItem(item.id)}><i className=\"fas fa-trash-alt\"></i></td>\n        </tr>)\n    ): ( <tr><td colSpan={7}>vous n'avez encore rien commande</td></tr> )\n    return (\n        <div>\n            <h5>panier</h5>\n            <div className=\"container bg-white\">\n                <div className=\"row\">\n                    <div className=\"col-md-8\">\n                        <div className=\"table-container\">\n                            <table className=\"table\">\n                            <thead>\n                                <tr>\n                                <th scope=\"col\"><input type=\"checkbox\" checked={check} onClick={e=>checkAll(e)} /></th>\n                                <th scope=\"col\">image</th>\n                                <th scope=\"col\">Article</th>\n                                <th scope=\"col\">description</th>\n                                <th scope=\"col\">Qté</th>\n                                <th scope=\"col\">Prix</th>\n                                <th scope=\"col\">Action</th>\n                                </tr>\n                            </thead>\n                            <tbody>\n                                {listItem}\n                            </tbody>\n                            </table>\n                        </div>\n                        <hr/>\n                        <div className=\"row\">\n                            <div className=\"col-md-4 mb-3\"><button type=\"button\" className=\"btn btn-warning\" onClick={()=> nav.push('/home')}><i className=\"fas fa-cart-plus\"></i> Continuer les achats</button></div>\n                            <div className=\"col-md-4 mb-3\"> <button type=\"button\" className=\"btn btn-secondary\" onClick={delAll}><i className=\"fas fa-trash\"></i> Tout Supprimer</button></div>\n                        </div>\n                    </div>\n                    <div className=\"col-md-3 resume\">\n                        <h4>RESUME</h4>\n                        <ul className=\"list-group\">\n                            <li className=\"list-group-item d-flex justify-content-between align-items-center\">\n                                Nombre D'articles\n                                <span className=\"badge badge-primary badge-pill\"> {comand.items} </span>\n                            </li>\n                            <li className=\"list-group-item d-flex justify-content-between align-items-center\">\n                                Montant total\n                                <span className=\"badge badge-primary badge-pill\"> {total()} Fcfa </span>\n                            </li>\n                            <li className=\"list-group-item d-flex justify-content-between align-items-center\">\n                                Taxe\n                                <span className=\"badge badge-primary badge-pill\"> 0 Fcfa </span>\n                            </li>\n                            <li className=\"list-group-item d-flex justify-content-between align-items-center\">\n                                Total de la commande\n                                <span className=\"badge badge-primary badge-pill\"> {total()-taxe} Fcfa </span>\n                            </li>\n                            <button className=\"btn btn-secondary colorbtncommander\" onClick={sendComand} disabled={toggle()}><b>COMMANDER</b></button>\n                        </ul>\n                    </div>\n                </div>\n            </div>\n        </div>\n    );\n\n}\n\nconst mapStateToProps=(state: any)=>({\n    comand: state.comand,\n})\n\nexport default connect(mapStateToProps)(Panier);","/media/bbh/@Nkul/PROJET_CLASSE_X/innov237/consoInvest_web/src/component/pages/Product/ProductDetail/ProductDetail.tsx",["197"],"import React from 'react';\nimport './ProductDetail.css'\n\nconst ProductDetail: React.FC = () => {\n    return (\n        <div className=\"container\">\n            <div className=\"row container-fluid\">\n                <div className=\"col-md-6\">\n                    <div className=\"row\">\n                        <div className=\"col-12\">\n                            <img width=\"100%\" src=\"https://global2019-static-cdn.kikuu.com/upload-productImg-1557817317157_320_234.jpeg?\" />\n                        </div>\n                    </div>\n                </div>\n\n                <div className=\"col-md-6 product__detail__row\">\n                    <p className=\"product__title\">Chausure homme</p>\n                    <p className=\"product__price\">50.000 FCFA</p>\n                    <p className=\"product__decription\">Lorem ipsum dolor sit amet consectetur adipisicing elit. Recusandae hic alias nemo illo, illum laboriosam libero similique at, deleniti quo repudiandae voluptatem? Aut qui, iusto quia ipsam nisi dolor incidunt!</p>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default ProductDetail;","/media/bbh/@Nkul/PROJET_CLASSE_X/innov237/consoInvest_web/src/component/pages/User/Login/Login.tsx",["198","199"],"import React from \"react\";\nimport { Link, useHistory } from \"react-router-dom\";\nimport TextInput from \"../../../layout/FormInput/TextInput\";\nimport './Login.css';\nimport { useForm } from \"react-hook-form\";\nimport ApiService from \"../../../../services/ApiService\";\n\nimport {useDispatch} from 'react-redux'\n\nimport { LOGIN_ACTION,SHOP_ACTION} from '../../../../store/authReducers'\n\nconst Login: React.FC = () => {\n\n    const history = useHistory();\n    const Api = new ApiService();\n\n    const dispatch = useDispatch()\n\n    const { register, handleSubmit, watch, errors } = useForm();\n    const [error, setError] = React.useState('')\n\n    const getShop = async (id:any) => {\n        var response = await Api.getData(\"getUserShop?id_user=\"+id);\n        if (response.data.length)\n            dispatch(SHOP_ACTION(response.data[0]))\n            \n        history.push(\"/account\");\n        \n    }\n    const onSubmit = async (data: any) => {\n        setError('')\n        var response = await Api.postData(\"login\", data);\n        console.log(response.data);\n\n        if (response.data['success']) {\n            \n            dispatch(LOGIN_ACTION(response.data['data'].id))\n            localStorage.setItem(\"authUserData\", JSON.stringify(response.data['data']));\n            getShop(response.data['data'].id)\n            //\n        } else {\n            setError(response.data['message'])\n            console.log(response.data['message']);\n        }\n    };\n\n\n    return (\n        <div>\n            <div className=\"card container-fluid d-flex justify-content-center align-items-center mt-2\">\n                <div className=\"container\">\n                    <form className=\"form p-5\" onSubmit={handleSubmit(onSubmit)} >\n                        <h2>Connexion</h2>\n                        <p className=\"form__subtitle\">Bon retour sur ConsoInvest</p>\n\n                        <TextInput type=\"email\" name=\"email\" placeholder=\"Email\" required={true} refs={register({ required: true })} />\n                        <TextInput type=\"password\" name=\"password\" placeholder=\"Mot de passe\" required={true} refs={register({ required: true })} />\n\n                        <div className=\"form-group\">\n                            <input type=\"submit\" className=\"btn btn-primary w-100\" value=\"Me connecter\" />\n                        </div>\n                        <div className=\"form-group\">\n                            <p style={{textAlign: 'center', color: 'red',fontWeight: 'bold'}}>{error}</p>\n                        </div>\n                        \n                        <div className=\"form-group\">\n                            <p><Link to=\"/register\"> J'ai déjà un compte ? M'inscrire</Link></p>\n                        </div>\n                        <h5 className=\"text-center\"> <Link to=\"/home\"> Accueil</Link></h5>\n                    </form>\n                </div>\n            </div>\n        </div >\n    );\n\n}\n\nexport default Login;","/media/bbh/@Nkul/PROJET_CLASSE_X/innov237/consoInvest_web/src/component/pages/User/Account/Home/Home.tsx",["200"],"import React from \"react\";\nimport { Link, Switch, Route } from \"react-router-dom\";\nimport './Home.css';\nimport Sidebar from '../../../../layout/Sidebar/UserSideBar';\n\nimport Commande from '../Commande/Commande';\nimport Affiliers from '../Affiliers/Affiliers';\nimport Password from '../Password/Password';\nimport Profil from '../Profil/Profil';\nimport Boutique from '../Boutique/Boutique';\nimport Epagne from '../Epagne/Epagne';\n\n\n\nconst Home: React.FC = () => {\n\n\n\n    return (\n        <div>\n            <nav className=\"navbar navbar-light sticky-top bg-white flex-md-nowrap p-0 shadow\">\n                <button className=\"navbar-toggler position-absolute d-md-none collapsed\" type=\"button\" data-toggle=\"collapse\" data-target=\"#sidebarMenu\" aria-controls=\"sidebarMenu\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">\n                <i className=\"fas fa-cog fa-spin\"></i>\n                </button>\n            </nav>\n            <div className=\"container\">\n                <div className=\"row\">\n                    <div className=\"col-md-2 no-padding left__menu\">\n                        <Sidebar />\n                    </div>\n                    <div className=\"col-md-8 p-3\">\n                        <Switch>\n                            <Route path=\"/account\" exact>\n                                <Profil />\n                            </Route>\n                        </Switch>\n                        <Switch>\n                            <Route path=\"/account/user\">\n                            <Sidebar />\n                            </Route>\n                        </Switch>\n                        <Switch>\n                            <Route path=\"/account/commande\">\n                                <Commande />\n                            </Route>\n                        </Switch>\n                        <Switch>\n                            <Route path=\"/account/affiliers\">\n                                <Affiliers />\n                            </Route>\n                        </Switch>\n                        <Switch>\n                            <Route path=\"/account/password\">\n                                <Password />\n                            </Route>\n                        </Switch>\n                        <Switch>\n                            <Route path=\"/account/boutique\">\n                                <Boutique />\n                            </Route>\n                        </Switch>\n                        <Switch>\n                            <Route path=\"/account/epagne\">\n                                <Epagne />\n                            </Route>\n                        </Switch>\n                        \n                </div>\n\n                <div className=\"col-md-2  p-3\">\n                    <div className=\"my-3 p-3 bg-white rounded shadow-sm\">\n                        <p>informations de l'utilisateurs</p>\n                    </div>\n                </div>\n                </div>\n            </div>\n        </div>\n\n    );\n\n}\n\n/********** */\n\nexport default Home;","/media/bbh/@Nkul/PROJET_CLASSE_X/innov237/consoInvest_web/src/component/pages/User/Register/Register.tsx",["201"],"import React, { useState } from \"react\";\nimport { Link, useHistory } from \"react-router-dom\";\nimport TextInput from \"../../../layout/FormInput/TextInput\";\nimport './Register.css';\nimport { useForm } from \"react-hook-form\";\nimport ApiService from \"../../../../services/ApiService\";\n\nconst Register: React.FC = () => {\n\n    const { register, handleSubmit, watch, errors } = useForm();\n    const [enableCodeInvitationInput, setCodeInputStatus] = useState(false);\n    const Api = new ApiService();\n    const history = useHistory()\n\n\n    const handleconnuMode = (e: any) => {\n\n        if (e.target.value === 'proche') {\n            setCodeInputStatus(true);\n        }else{\n            setCodeInputStatus(false); \n        }\n    }\n\n    const onSubmit = async (data: any) => {\n        \n\n        var response = await Api.postData(\"inscription\", data);\n        if (response.data['success']) {\n            alert(response.data['message']);\n            history.push('/login')\n            \n        } else {\n            alert(response.data['message']);\n        }\n    };\n\n\n\n    return (\n        <div>\n            <div className=\"card container-fluid d-flex justify-content-center align-items-center mt-2\">\n                <div className=\"container\">\n                    <form className=\"form p-5\" onSubmit={handleSubmit(onSubmit)}>\n                        <h2>Inscription</h2>\n                        <p className=\"form__subtitle\">Inscrivez vous gratuitement!</p>\n\n                        <TextInput type=\"text\" name=\"nom\" placeholder=\"Nom et prenom\" required={true} refs={register({ required: true })} />\n                        {errors.nom && <span>Nom obligatoire</span>}\n\n                        <TextInput type=\"email\" name=\"email\" placeholder=\"Email\" required={true} refs={register({ required: true })} />\n                        <TextInput type=\"tel\" name=\"telephone\" placeholder=\"+237 téléphone\" required={true} refs={register({ required: true })} />\n                        <span className=\"helper\">De preference un numero whatsapp</span>\n\n                        <div className=\"form-group\">\n                            <select name=\"ville\" className=\"form-control\">\n                                <option selected disabled>Ville</option>\n                                <option>yaoundé</option>\n                                <option>douala</option>\n                            </select>\n                        </div>\n\n                        <div className=\"form-group\">\n                            <label>Comment avez-vous connu consoInvest?</label>\n                            <select name=\"connuMode\" className=\"form-control\" ref={register({ required: true })} onChange={handleconnuMode} >\n                                <option selected disabled value=\"null\">choisi..</option>\n                                <option value=\"proche\">Via un proche</option>\n                                <option value=\"pub\">Via une publicité</option>\n                                <option value=\"autre\">Autre</option>\n                            </select>\n                        </div>\n\n                        {enableCodeInvitationInput && <TextInput type=\"text\" name=\"codeInvitation\" placeholder=\"Code invitation\" required={true} refs={register({ required: true })}></TextInput>}\n                        {errors.codeInvitation && <span>Code Obligatoire</span>}\n\n                        <TextInput type=\"password\" name=\"password\" placeholder=\"Mot de passe\" required={true} refs={register({ required: true })} />\n                        <TextInput type=\"password\" name=\"confirm-password\" placeholder=\"Confirmez votre mot de passe\" required={true} refs={register({ required: true })} />\n\n                        <div className=\"form-group\">\n                            <input type=\"submit\" className=\"btn btn-primary w-100 mt-2\" value=\"M'inscrire\" />\n                        </div>\n                        <div className=\"form-group\">\n                            <p><Link to=\"/login\"> Accueil j'ai déjà un compte ? Me connecter</Link></p>\n                        </div>\n                        <h5 className=\"text-center\"> <Link to=\"/home\"> Accueil</Link></h5>\n                    </form>\n                </div>\n            </div>\n        </div >\n    );\n\n}\n\n/********** */\n\nexport default Register;","/media/bbh/@Nkul/PROJET_CLASSE_X/innov237/consoInvest_web/src/component/pages/Home/Home.tsx",["202","203","204","205","206","207","208","209","210","211","212","213","214"],"import React, { useEffect, useState, useContext } from \"react\";\nimport { Link, useHistory } from \"react-router-dom\";\nimport { useSelector } from 'react-redux'\n\nimport OwlCarousel from 'react-owl-carousel';\n\nimport ApiContext from '../../../context/ApiContext'\nimport StorageContext from '../../../context/StorageContext'\nimport './Home.css';\nimport ProductCard from \"../../layout/Product/ProductCard/ProductCard\";\n\nimport 'owl.carousel/dist/assets/owl.carousel.min.css';\nimport 'owl.carousel/dist/assets/owl.theme.default.min.css';\n\nimport {useDispatch} from 'react-redux';\n\nimport {\n    ADD_KEY,\n    ITEM_ACTION\n} from '../../../store/searchReducers'\n\nconst Home: React.FC = (props) => {\n\n\n    const Api: any = useContext(ApiContext);\n    const local: any=useContext(StorageContext);\n    const history = useHistory();\n    const [categorieData, setCategorie] = useState([]);\n    const [productData, setProduct] = useState([]);\n\n    const [boutiques, setBoutique] = useState([]);\n\n    const dispatch = useDispatch();\n    const comand = useSelector((state: any) => state.comand)\n    const search = useSelector((state: any) => state.search)\n    const auth = useSelector((state: any) => state.auth)\n\n\n    const redirect = (e:any) => {\n       \n        history.push({\n            pathname: '/shop',\n            search: '?uuid='+e.id\n        });\n     }\n    useEffect(() => {\n        \n        getCategorie();\n        getAllproduct();\n        \n        \n    }, []);\n\n    const getAllBoutique = async () =>  {\n        \n        //console.log(\"on va chercher sur le serveur\")\n        \n        var response = await Api.getData(\"getAllBoutique?lastInsertId=20&etat=1\");\n        if (response.status == 200) {\n            setBoutique(response.data);\n            //history.push(\"/login\", response.data); pour redirection les pages.\n        }\n    }\n\n    \n    const getCategorie = async () => {\n        \n        //console.log(\"on va chercher sur le serveur\")\n        var response = await Api.getData(\"getcategorie\");\n        if (response.status == 200) {\n            setCategorie(response.data);\n            //history.push(\"/login\", response.data); pour redirection les pages.\n        }\n    }\n    \n    const getAllproduct = async (url:any = null) => {\n        setProduct([])\n        if (url) {\n            var response = await Api.postData(\"getPublicationByCategorie\",{idCategorie:url, lastInsertId:10});\n                if (response.status == 200) \n                    setProduct(response.data)\n        }else{\n            var response = await Api.getData(\"getAllPublication?lastInsertId=\" + 100);\n                if (response.status == 200) \n                    setProduct(response.data)\n        }\n        \n \n    }\n\n    const toArray = (data: any) => {\n        var array = JSON.parse(data);\n        return array;\n    }\n\n    const openDetail = (product: any) => {\n        \n        dispatch(ITEM_ACTION(product))\n        history.push({\n            pathname: '/produit',\n            search: '?slug='+product.slug\n        });\n\n    }\n\n    const display = () => {\n\n        return (boutiques.length>1) ? (boutiques.length == 2) ? 2 :3  : 1\n    }\n\n  \n    return (\n        <div className=\"container\">\n            <div className=\"row\">\n                <div className=\"col-md-2 no-padding left__menu bd-red\">\n                    <div className=\"menu__title\">Catégorie</div>\n                    <div className=\"\">\n                        {categorieData.map((categorie, index) => (<div className=\"categirie__item\" key={index} onClick={() => getAllproduct(categorie['id'])}>\n                            <img src={\"./images/\" + categorie['icon_categorie']} className=\"categorie__image\" />  {categorie['libelle_categorie']}\n                        </div>))}\n                    </div>\n                </div>\n                <div className=\"col-md-8 no-padding\">\n                    <OwlCarousel\n                                            className=\"owl-theme\"\n                                            items={1}\n                                            dots={true}\n                                            dotClass=\"dot-div\"\n                                            dotsSpeed={true}\n                                            autoplay={true}\n                                            autoplaySpeed={500}\n                                            loop={true}\n                                        >\n                                            <div className=\"item\">\n                                                <div className=\"slider-content\">\n                                                    <img src=\"https://img.alicdn.com/tfs/TB1FFhgpMgP7K4jSZFqXXamhVXa-990-400.png\" className=\"slider__img\" alt=\"\" />\n                                                </div>\n                                            </div>\n                                            <div className=\"item\">\n                                                <div className=\"slider-content\">\n                                                    <img src=\"https://img.alicdn.com/tfs/TB1x8x9eEY1gK0jSZF